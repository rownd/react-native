    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "concurrently": "^7.1.0",
    "expo-cli": "^5.4.6",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.3.5"
