    "debounce": "^1.2.1",
    "expo": "~44.0.0",
    "expo-dev-client": "~0.8.5",
    "expo-linking": "^3.0.0",
    "expo-secure-store": "^11.1.1",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "jwt-decode": "^3.1.2",
    "ky": "^0.30.0",
    "lodash-es": "^4.17.21",
    "phone": "^3.1.15",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
