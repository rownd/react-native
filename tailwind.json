{
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"hidden": {
		"style": {
			"display": "none"
		}
	},
	"bg-white": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(255 255 255 / var(--tw-bg-opacity))"
		}
	},
	"py-8": {
		"style": {
			"paddingTop": 32,
			"paddingBottom": 32
		}
	},
	"py-6": {
		"style": {
			"paddingTop": 24,
			"paddingBottom": 24
		}
	},
	"pl-1": {
		"style": {
			"paddingLeft": 4
		}
	},
	"text-base": {
		"style": {
			"fontSize": 16,
			"lineHeight": 24
		}
	},
	"italic": {
		"style": {
			"fontStyle": "italic"
		}
	},
	"text-rose-800": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(159 18 57 / var(--tw-text-opacity))"
		}
	},
	"dark:bg-slate-900": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(15 23 42 / var(--tw-bg-opacity))"
		},
		"media": "(prefers-color-scheme: dark)"
	}
}